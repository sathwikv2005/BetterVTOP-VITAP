{
  "name": "bettervtop",
  "version": "1.2.0",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start",
    "start:clear": "npx expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "prebuild:dev": "APP_VARIANT=dev npx expo prebuild",
    "prebuild:dev:clean": "APP_VARIANT=dev npx expo prebuild --clean",
    "prebuild:android:dev": "APP_VARIANT=dev npx expo prebuild --platform android",
    "prebuild:android": "npx expo prebuild --platform android",
    "buildapk": "cd ./android && ./gradlew assembleRelease",
    "buildapk:clean": "cd ./android && ./gradlew clean assembleRelease",
    "buildapk:dev": "cd ./android && ./gradlew assembleDebug",
    "buildapk:dev:clean": "cd ./android && ./gradlew clean assembleDebug"
  },
